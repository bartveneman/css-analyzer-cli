{
  "name": "wallace-cli",
  "version": "1.2.0",
  "description": "Stylesheet analysis CLI Tool",
  "homepage": "https://www.projectwallace.com/oss",
  "scripts": {
    "test": "xo"
  },
  "bin": {
    "wallace": "./cli.js"
  },
  "keywords": [
    "stylesheets",
    "css",
    "analytics",
    "analysis",
    "stats",
    "statistics",
    "cli",
    "cli-app",
    "cssstats",
    "parker",
    "stylestats",
    "projectwallace",
    "wallace",
    "performance",
    "property"
  ],
  "author": "Bart Veneman",
  "license": "MIT",
  "dependencies": {
    "@projectwallace/css-analyzer": "^2.0.0-0",
    "chalk": "^2.4.1",
    "cli-table3": "^0.5.1",
    "cli-truncate": "^1.1.0",
    "get-css": "^1.2.7-2",
    "get-stdin": "^6.0.0",
    "is-absolute-url": "^2.1.0",
    "left-pad": "^1.3.0",
    "meow": "^5.0.0",
    "ora": "^3.0.0",
    "pretty-bytes": "^5.1.0",
    "term-size": "^1.2.0",
    "tinycolor2": "^1.4.1",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "prettier": "^1.15.3",
    "xo": "^0.23.0"
  },
  "xo": {
    "semicolon": false,
    "prettier": true
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
